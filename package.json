{
  "name": "2025-08-agent-mode-presentation",
  "version": "1.0.0",
  "description": "Presentation about Agentic Coding Tools and Agent Mode",
  "main": "index.js",
  "scripts": {
    "build": "marp slides.md --pdf --theme themes/nord-theme.css --no-config -o dist/presentation.pdf",
    "build:html": "marp slides.md --html --theme themes/nord-theme.css --no-config -o dist/presentation.html",
    "build:pptx": "marp slides.md --pptx --theme themes/nord-theme.css --no-config -o dist/presentation.pptx",
    "build:all": "npm run build:html && npm run build && npm run build:pptx",
    "dev": "marp --preview --theme themes/nord-theme.css --no-config slides.md",
    "watch": "marp --watch --theme themes/nord-theme.css --no-config slides.md -o dist/presentation.html",
    "server": "marp --server --no-config ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "marp",
    "presentation",
    "ai",
    "agent",
    "coding"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.3"
  }
}
